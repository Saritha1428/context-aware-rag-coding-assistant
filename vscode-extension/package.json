{
    "name": "rag-ai-assistant",
    "displayName": "RAG AI Coding Assistant",
    "description": "AI Assistant using RAG to analyze local project code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "rag-ai-sidebar",
                    "title": "RAG Assistant",
                    "icon": "$(comment-discussion)"
                }
            ]
        },
        "views": {
            "rag-ai-sidebar": [
                {
                    "type": "webview",
                    "id": "ragChatSidebar",
                    "name": "Chat with Project"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/node": "18.x",
        "@types/vscode": "^1.85.0",
        "@typescript-eslint/eslint-plugin": "^6.19.1",
        "@typescript-eslint/parser": "^6.19.1",
        "eslint": "^8.56.0",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "axios": "^1.13.5"
    }
}
